#!/usr/bin/ruby -Ku
#告訴状-2013-金沢地方検察庁御中_API
tfile=File.open('t').read.split("\n")
tdata=tfile.each_slice(3).to_a
tdata.reverse!

pfile=File.open('p').read
p=pfile.split(/<\/a>/)

pdata=Array.new
p.each do |d|
	pdata << d
end

if tdata.size == pdata.size then
#	puts "#{tdata.size}   ---------------   #{pdata.size}"
	tdata.size.times do |i|
		puts "<div>"
		puts pdata[i]
		puts tdata[i]
		puts "</div>\n"
	end
else
	puts "一致しないです。"
end


