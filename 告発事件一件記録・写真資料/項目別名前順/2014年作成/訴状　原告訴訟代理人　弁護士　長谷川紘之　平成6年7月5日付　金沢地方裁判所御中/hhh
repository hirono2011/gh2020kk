  726  ls
  727  ls -lh
  728  cd Videos/
  729  ls Screencast\ 2014-06-10\ 08\:52\:38.mp4 
  730  fn='Screencast 2014-06-10 08:52:38.mp4'
  731  echo $fn
  732  echo $fn|sed 's/\(Screencast\)/`date`/'
  733  echo $fn|sed 's/.*\([1-12][0-9][0-9][0-9]-\)/\1年/'
  734  echo $fn|sed 's/\(.*\)\([1-12][0-9][0-9][0-9]-\)/\1年/'
  735  echo $fn|sed 's/\(.*\)\([1-12][0-9][0-9][0-9]-\)/\1\2年/'
  736  echo $fn|sed 's/\(.*\)\([1-12][0-9][0-9][0-9]\)/\1\2年/'
  737  echo $fn|sed 's/\(.*\)\([1-12][0-9][0-9][0-9]\)-/\1\2年/'
  738  echo $fn|sed 's/\(.*\) \([1-12][0-9][0-9][0-9]\)-/\1\2年/'
  739  echo $fn|sed 's/\(.*\) \([1-12][0-9][0-9][0-9]\)-\([0-9][0-9]\)-\([0-9][0-9]\)/\1\2年\3月\4日/'
  740  echo $fn|sed 's/\(.*\) \([1-12][0-9][0-9][0-9]\)-\([0-9][0-9]\)-\([0-9][0-9]\) [0-9][0-9]\):\([0-9][0-9]\):\(.*\)\([0-9][0-9]\)/\1\2年\3月\4日\5時\6分\7秒\8/'
  741  echo $fn|sed 's/\(.*\) \([1-12][0-9][0-9][0-9]\)-\([0-9][0-9]\)-\([0-9][0-9]\) \([0-9][0-9]\):\([0-9][0-9]\):\(.*\)\([0-9][0-9]\)/\1\2年\3月\4日\5時\6分\7秒\8/'
  742  vi h-date-rename.sh
  743  chmod 700 h-date-rename.sh 
  744  ./h-date-rename.sh Screencast\ 2014-06-10\ 08\:52\:38.mp4 
  745  vi h-date-rename.sh
